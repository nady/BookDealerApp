<?php

namespace Backend\APIBundle\Entity;

/**
 * BookDealer
 */
class BookDealer
{
    /**
     * @var integer
     */
    private $status;

    /**
     * @var \DateTime
     */
    private $createdAt;

    /**
     * @var \DateTime
     */
    private $updatedAt;

    /**
     * @var \Backend\APIBundle\Entity\Book
     */
    private $book;

    /**
     * @var \Backend\APIBundle\Entity\Dealer
     */
    private $dealer;


    /**
     * Set status
     *
     * @param integer $status
     *
     * @return BookDealer
     */
    public function setStatus($status)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return integer
     */
    public function getStatus()
    {
        return $this->status;
    }

    /**
     * Set createdAt
     *
     * @param \DateTime $createdAt
     *
     * @return BookDealer
     */
    public function setCreatedAt($createdAt)
    {
        $this->createdAt = new \DateTime();

        return $this;
    }

    /**
     * Get createdAt
     *
     * @return \DateTime
     */
    public function getCreatedAt()
    {
        return $this->createdAt;
    }

    /**
     * Set updatedAt
     *
     * @param \DateTime $updatedAt
     *
     * @return BookDealer
     */
    public function setUpdatedAt($updatedAt)
    {
        $this->updatedAt = new \DateTime();

        return $this;
    }

    /**
     * Get updatedAt
     *
     * @return \DateTime
     */
    public function getUpdatedAt()
    {
        return $this->updatedAt;
    }

    /**
     * Set book
     *
     * @param \Backend\APIBundle\Entity\Book $book
     *
     * @return BookDealer
     */
    public function setBook(\Backend\APIBundle\Entity\Book $book)
    {
        $this->book = $book;

        return $this;
    }

    /**
     * Get book
     *
     * @return \Backend\APIBundle\Entity\Book
     */
    public function getBook()
    {
        return $this->book;
    }

    /**
     * Set dealer
     *
     * @param \Backend\APIBundle\Entity\Dealer $dealer
     *
     * @return BookDealer
     */
    public function setDealer(\Backend\APIBundle\Entity\Dealer $dealer)
    {
        $this->dealer = $dealer;

        return $this;
    }

    /**
     * Get dealer
     *
     * @return \Backend\APIBundle\Entity\Dealer
     */
    public function getDealer()
    {
        return $this->dealer;
    }

    /**
     * @var integer
     */
    private $id;


    /**
     * Set id
     *
     * @param integer $id
     *
     * @return BookDealer
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
